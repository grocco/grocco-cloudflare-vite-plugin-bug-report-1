{
    "include": [
        "src/**/*",
        "worker-configuration.d.ts"
      ],
    "compilerOptions": {
        "composite": true,
        "lib": ["DOM", "DOM.Iterable", "ES2022"],
        "types": [ "vite/client"],
        "target": "ES2022",
        "module": "ES2022",
        "moduleResolution": "bundler",
      "checkJs": true,
      "verbatimModuleSyntax": true,
      "skipLibCheck": true,
      "strict": true,
      "noEmit": true,
    }
  }
  